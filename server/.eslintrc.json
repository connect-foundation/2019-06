{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": 0,
    "no-trailing-spaces": 0,
    "no-unexpected-multiline": 0,
    "comma-dangle": 0,
    "next-line": 0,
    "object-curly-newline": 0,
    "max-len": ["error", 100],
    "arrow-parens": 0,
    "no-restricted-syntax": 0
  }
}
